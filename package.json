    "@vue/compiler-sfc": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "chalk": "^4.1.0",
    "vue-eslint-parser": "^7.3.0",
    "eslint": "^7.17.0",
